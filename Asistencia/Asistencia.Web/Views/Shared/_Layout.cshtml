        let rutaLogin = '@Url.Action("Login", "Autenticacion")';
        let rutaIndex = '@Url.Action("Index", "Home")';
    </script>
    <script src="~/js/Utility/Constans.js"></script>
    <script src="~/js/Utility/Functions.js"></script>
    <!--Vue js -->
    <script src="~/js/Vue/handlebars.min.js"></script>
    <!-- vue js -->
