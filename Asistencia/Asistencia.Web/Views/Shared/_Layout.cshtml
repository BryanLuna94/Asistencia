<body class="fix-header">

    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>

    <div id="app">

        <v-layoutheader></v-layoutheader>
        <v-leftsidebar></v-leftsidebar>

        <div id="page-wrapper">

            <div class="container-fluid">

                @await Html.PartialAsync("_LayoutTitle")

                @RenderBody()

                @await Html.PartialAsync("_LayoutRightSidebar")

            </div>

        </div>

        <v-layoutfooter></v-layoutfooter>

    </div>

    <script src="~/AmpleAdmin/plugins/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/AmpleAdmin/bootstrap/dist/js/bootstrap.min.js"></script>
